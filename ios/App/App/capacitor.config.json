{
	"appId": "com.ionicframework.fyle595781",
	"appName": "Fyle",
	"webDir": "www",
	"loggingBehavior": "debug",
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false,
			"androidScaleType": "CENTER_CROP"
		},
		"Keyboard": {
			"style": "LIGHT"
		}
	},
	"cordova": {
		"accessOrigins": [
			"https://app.fylehq.com"
		]
	},
	"server": {
		"androidScheme": "http"
	},
	"packageClassList": [
		"CameraPreview",
		"AppPlugin",
		"CAPBrowserPlugin",
		"CAPCameraPlugin",
		"ClipboardPlugin",
		"DevicePlugin",
		"GeolocationPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"CAPNetworkPlugin",
		"PreferencesPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"TextZoomPlugin",
		"NativeSettingsPlugin",
		"SecureStoragePlugin",
		"CDVPlugin"
	]
}
