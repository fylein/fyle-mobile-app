{
	"appId": "com.ionicframework.fyle595781",
	"appName": "Sage Exp Mgmt",
	"webDir": "www",
	"loggingBehavior": "debug",
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false,
			"androidScaleType": "CENTER_CROP"
		},
		"Keyboard": {
			"style": "LIGHT",
			"resizeOnFullScreen": true
		},
		"PushNotifications": {
			"presentationOptions": [
				"alert",
				"sound",
				"badge"
			]
		},
		"AppShortcuts": {
			"shortcuts": [
				{
					"id": "capture_receipt",
					"title": "Capture Receipt",
					"iosIcon": "camera",
					"androidIcon": "ic_shortcut_camera_sage"
				},
				{
					"id": "add_expense",
					"title": "Add Expense",
					"iosIcon": "plus.app",
					"androidIcon": "ic_shortcut_add_sage"
				}
			]
		}
	},
	"cordova": {
		"accessOrigins": [
			"https://app.fylehq.com"
		]
	},
	"server": {
		"androidScheme": "http"
	},
	"android": {
		"adjustMarginsForEdgeToEdge": "force"
	},
	"packageClassList": [
		"CameraPreview",
		"AppPlugin",
		"CAPBrowserPlugin",
		"CAPCameraPlugin",
		"ClipboardPlugin",
		"DevicePlugin",
		"GeolocationPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"CAPNetworkPlugin",
		"PreferencesPlugin",
		"PushNotificationsPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"TextZoomPlugin",
		"AppShortcutsPlugin",
		"FilePickerPlugin",
		"NativeSettingsPlugin",
		"SecureStoragePlugin",
		"CDVPlugin"
	]
}
