{
	"appId": "com.ionicframework.fyle595781",
	"appName": "Fyle",
	"webDir": "www",
	"bundledWebRuntime": false,
	"loggingBehavior": "debug",
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false,
			"androidScaleType": "CENTER_CROP"
		},
		"Keyboard": {
			"style": "LIGHT"
		},
		"PushNotifications": {
			"presentationOptions": [
				"alert"
			]
		}
	},
	"cordova": {
		"accessOrigins": [
			"https://app.fylehq.com"
		]
	}
}
